# Load Balancer Configuration
LB_PORT=80
LB_ALGO=rr
BE_SERVERS=[{"domain":"http://localhost:8081","weight":1},{"domain":"http://localhost:8082","weight":1}]
BE_RETRIES=3
BE_RETRY_DELAY=200
BE_PING_PATH=/ping
BE_PING_RETRIES=3
BE_PING_RETRY_DELAY=500
HEALTH_CHECK_INTERVAL=30000
SEND_ALERT_WEBHOOK=https://your-webhook-url.com
ALERT_ON_BE_FAILURE_STREAK=3
ALERT_ON_ALL_BE_FAILURE_STREAK=3
ENABLE_SELF_HEALING=true
TEST_ONLY_CHANCES_OF_HEALING_SERVER=0.5

# Sticky Sessions
ENABLE_STICKY_SESSION=true
STICKY_SESSION_COOKIE_NAME=SERVERID
STICKY_SESSION_COOKIE_MAX_AGE=3600000

# Caching
ENABLE_CACHE=true
CACHE_TTL=300

# Content-Based Routing
CONTENT_BASED_ROUTING=[{"path":"/api","server":"http://localhost:8081"},{"header":{"X-API-Version":"2"},"server":"http://localhost:8082"}]

# Regional Configuration
US_EAST_SERVER_1=http://us-east-1.example.com
US_EAST_SERVER_2=http://us-east-2.example.com
US_WEST_SERVER_1=http://us-west-1.example.com
US_WEST_SERVER_2=http://us-west-2.example.com
EU_WEST_SERVER_1=http://eu-west-1.example.com
EU_WEST_SERVER_2=http://eu-west-2.example.com
EU_CENTRAL_SERVER_1=http://eu-central-1.example.com
EU_CENTRAL_SERVER_2=http://eu-central-2.example.com
AP_SOUTHEAST_SERVER_1=http://ap-southeast-1.example.com
AP_SOUTHEAST_SERVER_2=http://ap-southeast-2.example.com
AP_NORTHEAST_SERVER_1=http://ap-northeast-1.example.com
AP_NORTHEAST_SERVER_2=http://ap-northeast-2.example.com
SA_EAST_SERVER_1=http://sa-east-1.example.com
SA_EAST_SERVER_2=http://sa-east-2.example.com
DEFAULT_REGION=us-east

# Geo-Routing
ENABLE_GEO_ROUTING=true
GEO_ROUTING_SERVICE=https://geo-routing-service.example.com

# Latency-Based Routing
ENABLE_LATENCY_BASED_ROUTING=true
LATENCY_CHECK_INTERVAL=300000

# Rate Limiting
ENABLE_RATE_LIMIT=true
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# SSL Configuration
ENABLE_SSL=false
SSL_CERT_PATH=/path/to/ssl/cert.pem
SSL_KEY_PATH=/path/to/ssl/key.pem

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Worker Processes
WORKER_PROCESSES=1

# Circuit Breaker
ENABLE_CIRCUIT_BREAKER=true
CIRCUIT_BREAKER_THRESHOLD=5
CIRCUIT_BREAKER_TIMEOUT=30000

# Request Timeout
REQUEST_TIMEOUT=30000

# Custom Error Pages
CUSTOM_ERROR_PAGES={"404":"/path/to/404.html","500":"/path/to/500.html"}

# WebSocket Support
ENABLE_WEBSOCKET=true

# CORS Configuration
ENABLE_CORS=true
CORS_ORIGIN=*
CORS_METHODS=GET,HEAD,PUT,PATCH,POST,DELETE

# Compression
ENABLE_COMPRESSION=true
COMPRESSION_LEVEL=6

# IP Filtering
IP_WHITELIST=192.168.1.1,192.168.1.2
IP_BLACKLIST=10.0.0.1,10.0.0.2

# Request/Response Transformation
ENABLE_REQUEST_TRANSFORM=true
ENABLE_RESPONSE_TRANSFORM=true

# API Key Authentication
ENABLE_API_KEY_AUTH=true
API_KEYS=key1,key2,key3

# Monitoring
ENABLE_MONITORING=true
MONITORING_PATH=/monitoring

# Dynamic Configuration
ENABLE_DYNAMIC_CONFIG=true
DYNAMIC_CONFIG_INTERVAL=60000

# Graceful Shutdown
GRACEFUL_SHUTDOWN_TIMEOUT=30000